<div>
  <h1>Lista de productos</h1>

    <ul>
        {{#each productList}}
        <li>
            <a href="/product/{{this._id}}/edit">{{this.name}}</a>
            {{this.description}}
            <br>
            {{this.category}}
            {{this.supplier.name}}
        </li>

        <img src={{this.image}} alt="imagen-producto">
    </ul>
        {{/each}}

</div>